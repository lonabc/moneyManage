<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.yinfuxi.Mapper.UserMapper">
    <insert id="insertRegister" >
        insert into bestlove(age,birthday,name,school,password) values (#{age},#{birthday},#{name},#{school},#{password});
    </insert>
    <select id="selectAll" resultType="com.example.yinfuxi.StaticMy.User" >
        select * from moneyeveryday ;
    </select>
</mapper>